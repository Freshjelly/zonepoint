# Discord Webhooks
DISCORD_WEBHOOK_BEGINNER="your_webhook_url_here"
DISCORD_WEBHOOK_PRO="your_webhook_url_here"

# LLM Provider (anthropic or openai)
PROVIDER=openai

# API Keys
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Timezone (default: Asia/Tokyo)
TZ=Asia/Tokyo

# Optional: Log Level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO